# Server
PORT=4000
NODE_ENV=development

# Database (PostgreSQL)
# For Docker: postgresql://gps_user:gps_password@postgres:5432/gps_mock_db
# For Local:  postgresql://gps_user:gps_password@localhost:5432/gps_mock_db
DATABASE_URL=postgresql://gps_user:gps_password@localhost:5432/gps_mock_db

# Redis
REDIS_URL=redis://localhost:6379

# JWT
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=7d

# Default Admin
DEFAULT_ADMIN_PASSWORD=admin123